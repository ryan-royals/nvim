{
  "tf-m-naming": {
    "prefix": "tf-m-naming",
    "body": [
      "module \"naming\" {",
      "    source = \"Azure\/naming\/azurerm\"",
      "    version = \"0.4.2\"",
      "",
      "    prefix = ${1:[\"prefix\"]}",
      "    suffix = ${2:[\"suffix\"]}",
      "  }"
    ]
  },
  "tf-rp-azapi": {
    "body": [
      "azapi = {",
      "    source  = \"azure\/azapi\"",
      "    version = \"${1:~> 2.0}\"",
      "  }"
    ],
    "prefix": "tf-rp-azapi"
  },
  "tf-rp-time": {
    "body": [
      "time = {",
      "    source  = \"hashicorp\/time\"",
      "    version = \"${1:~> 0.0}\"",
      "  }"
    ],
    "prefix": "tf-rp-time"
  },
  "tf-rp-azuread": {
    "body": [
      "azuread = {",
      "    source  = \"hashicorp\/azuread\"",
      "    version = \"${1:~> 3.0}\"",
      "  }"
    ],
    "prefix": "tf-rp-azuread"
  },
  "tf-rp-azurerm": {
    "body": [
      "azurerm = {",
      "    source  = \"hashicorp\/azurerm\"",
      "    version = \"${1:~> 4.0}\"",
      "  }"
    ],
    "prefix": "tf-rp-azurerm"
  },
  "tf-rp-random": {
    "body": [
      "random = {",
      "    source  = \"hashicorp\/random\"",
      "    version = \"${1:~> 3.0}\"",
      "  }"
    ],
    "prefix": "tf-rp-random"
  }
}

